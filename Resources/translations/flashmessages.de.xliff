<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file date="2019-01-12T20:00:00Z" source-language="en" target-language="de" datatype="plaintext" original="flashmessages.en.xliff">
        <body>
            <trans-unit id="timesheet.error">
                <source>timesheet.error</source>
                <target>Es ist ein Fehler aufgetreten: %reason%</target>
            </trans-unit>
            <trans-unit id="timesheet.pause.success">
                <source>timesheet.pause.success</source>
                <target>Zeitmessung wurde pausiert</target>
            </trans-unit>
            <trans-unit id="timesheet.pause.error">
                <source>timesheet.pause.error</source>
                <target>Zeitmessung konnte nicht pausiert werden: %reason%</target>
            </trans-unit>
            <trans-unit id="timesheet.resume.success">
                <source>timesheet.resume.success</source>
                <target>Zeitmessung wurde wieder aufgenommen</target>
            </trans-unit>
            <trans-unit id="timesheet.resume.error">
                <source>timesheet.resume.error</source>
                <target>Zeitmessung konnte nicht wieder aufgenommen werden: %reason%</target>
            </trans-unit>
            <trans-unit id="timesheet.start-activity.success">
                <source>timesheet.start-activity.success</source>
                <target>Zeitmessung für den Auftrag wurde gestartet</target>
            </trans-unit>
            <trans-unit id="timesheet.start-activity.error">
                <source>timesheet.start-activity.error</source>
                <target>Zeitmessung für den Auftrag konnte nicht durchgeführt werden: %reason%</target>
            </trans-unit>
            <trans-unit id="timesheet.warning.modify-active">
                <source>timesheet.warning.modify-active</source>
                <target>Achtung! Sie bearbeiten gerade den Eintrag einer aktuellen Aufzeichnung. Dies kann zu unerwarteten Fehlern führen.</target>
            </trans-unit>
            <trans-unit id="recent-activity.reset.success">
                <source>recent-activity.reset.success</source>
                <target>Ihre Letzte Buchung wurde zurückgesetzt. Ihre aktuelle Zeitaufzeichnung wurde gestoppt.</target>
            </trans-unit>
       </body>
    </file>
</xliff>
