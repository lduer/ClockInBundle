{
    "name": "lduer/clock-in-bundle",
    "description": "This Kimai 2 plugin provides additional features for a simpler ClockIn usage on mobile screens.",
    "homepage": "https://github.com/lduer/ClockInBundle",
    "type": "kimai-plugin",
    "version": "0.2",
    "require": {
        "kimai/kimai2-composer": "*"
    },
    "keywords": [
        "kimai",
        "kimai-plugin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "lduer",
            "email": "duerlukas@gmail.com"
        }
    ],
    "extra": {
        "kimai": {
            "require": "1.6",
            "version": "0.2",
            "name": "ClockInBundle"
        }
    },
    "scripts": {
        "codestyle": "vendor/bin/php-cs-fixer fix --dry-run --verbose --show-progress=none",
        "codestyle-fix": "vendor/bin/php-cs-fixer fix"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
